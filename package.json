{
  "name": "readme-parrot",
  "description": "A simple little README template.",
  "author": "Lena Schnedlitz",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "analyse:watch": "svelte-check --watch",
    "analyse": "svelte-check",
    "test": "jest",
    "lint": "eslint src/**/*",
    "prettify": "prettier --write src/**/* public/*.{html,css} *.{config.js,json,yml,md}",
    "prepare": "husky install",
    "deploy": "npm run build && npx gh-pages --dist public --dotfiles true"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.4",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/svelte": "3.0.3",
    "@tsconfig/svelte": "2.0.1",
    "@types/jest": "27.0.3",
    "@types/simplemde": "1.11.8",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "babel-jest": "27.4.4",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "jest": "27.4.4",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.1",
    "pretty-quick": "3.1.2",
    "rollup": "2.61.1",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.44.2",
    "svelte-check": "2.2.10",
    "svelte-jester": "2.1.5",
    "svelte-preprocess": "4.9.8",
    "ts-jest": "27.1.1",
    "tslib": "2.3.1",
    "typescript": "4.5.3"
  },
  "dependencies": {
    "easymde": "2.15.0",
    "sirv-cli": "1.0.14"
  }
}
